{
  "expo": {
    "name": "MangaMobileApp",
    "slug": "MangaMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstore-icon.png", 
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#673AB7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iabrahami.MangaMobileApp",
      "icon": "./assets/images/app_icon.png" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#673AB7"
      },
      "package": "com.iabrahami.MangaMobileApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9bfc2ef-356c-465c-b3ef-937a127299c2"
      }
    }
  }
}
